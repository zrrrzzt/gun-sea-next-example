{
  "name": "gun-sea-next-example",
  "version": "1.0.2",
  "description": "Example app with GUN/SEA and Next.js",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/gun-sea-next-example.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/gun-sea-next-example/issues"
  },
  "homepage": "https://github.com/zrrrzzt/gun-sea-next-example#readme",
  "engines": {
    "node": ">=10.14.2"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "npm run export && now --static out --name gun-sea-next-example"
  },
  "keywords": [
    "gun",
    "sea",
    "next",
    "example"
  ],
  "dependencies": {
    "gun": "0.2020.520",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sea-canon": "git+https://github.com/zrrrzzt/sea-canon.git"
  },
  "devDependencies": {
    "ava": "3.8.2",
    "babel-eslint": "10.1.0",
    "coveralls": "3.1.0",
    "nyc": "15.0.1",
    "standard": "14.3.4"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
